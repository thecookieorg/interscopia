<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/jquery.mixitup.min.js"></script> 

<!-- FancyBox -->
<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/jquery.fancybox.pack.js"></script>
<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/owl.carousel.min.js"></script>
<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/jquery.plugin.min.js"></script>
<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/jquery.datepick.min.js"></script>
<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/wow.js"></script>
<script src="https://res.cloudinary.com/discovercity/raw/upload/interscopia/js/script.js"></script>

</body>
</html>
